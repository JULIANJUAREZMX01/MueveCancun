{
  "name": "temp-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "build": "npm run check-wasm && astro build",
    "astro": "astro",
    "lint": "eslint .",
    "test": "vitest",
    "build:wasm": "npm run build:wasm:route && npm run build:wasm:spatial",
    "build:wasm:route": "cd rust-wasm/route-calculator && wasm-pack build --target web --out-dir ../../public/wasm/route-calculator",
    "build:wasm:spatial": "cd rust-wasm/spatial-index && wasm-pack build --target web --out-dir ../../public/wasm/spatial-index",
    "preview": "vite preview",
    "check-wasm": "node -e \"const fs=require('fs'); ['route-calculator/route_calculator_bg.wasm', 'spatial-index/spatial_index_bg.wasm'].forEach(f => { if(!fs.existsSync('./public/wasm/' + f)) { console.error('❌ WASM binary missing: ' + f); process.exit(1); } }); console.log('✅ All WASM binaries found.');\""
  },
  "dependencies": {
    "astro": "^5.16.15",
    "@astrojs/react": "^4.2.1",
    "@astrojs/sitemap": "^3.2.1",
    "@tailwindcss/vite": "^4.1.18",
    "idb": "^8.0.3",
    "lucide-react": "^0.563.0",
    "mapbox-gl": "^3.18.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^4.0.18",
    "wasm-pack": "^0.14.0"
  }
}
